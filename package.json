{
  "name": "dotto-typespec",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/openapi": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/rest": "latest"
  },
  "devDependencies": {
    "@redocly/cli": "latest"
  },
  "scripts": {
    "build": "pnpm run compile && pnpm run docs:build",
    "compile": "tsp compile .",
    "docs:build": "redocly build-docs -o ./dist/announcement-api.html ./tsp-output/schema/AnnouncementService/openapi.AnnouncementService.yaml && redocly build-docs -o ./dist/app-bff-api.html ./tsp-output/schema/AppBFFService/openapi.AppBFFService.yaml",
    "docs:preview": "redocly preview -d ./tsp-output/schema/ -p 4041"
  }
}
